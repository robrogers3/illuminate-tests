{
    "name": "robrogers/validation",
    "license": "MIT",
    "description": "testing validaiton libs from illuminate",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "repositories": [
        {
            "type":"vcs",
            "url": "https://github.com/robrogers3/translation.git"
