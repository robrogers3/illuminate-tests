    "require": {
        "php": ">=5.3.0",
        "slim/slim": "2.*",
	"zeuxisoo/slim-whoops": "0.2.0",
	"filp/whoops": "1.1.2",
        "illuminate/container": "4.1.*",
        "illuminate/translation": "4.1.*",
        "illuminate/support": "4.1.*",
        "illuminate/validation": "4.1.*",
        "illuminate/cache": "4.1.*",        
        "symfony/http-foundation": "2.6.12",
        "symfony/translation": "2.6.12",
        "doctrine/cache": "1.5.*",
        "doctrine/common": "2.5.*",
        "camspiers/json-pretty": "~1.0.2",
        "doctrine/dbal" : "~2.5.0",
        "doctrine/doctrine-bundle": "~1.6.2",
        "doctrine/orm": "~2.4.8",
        "nesbot/carbon" : "~1.13.0",
        "netcarver/textile" : "~3.5.5",
        "robmorgan/phinx" : "0.4.1",
        "symfony/monolog-bundle": "~2.7.0",
        "bugsnag/bugsnag": "2.*",
        "vube/php-filesystem": "0.2",
        "touki/ftp": "~1.2",
        "patchwork/utf8": "~1.2",
        "swiftmailer/swiftmailer": "5.4.*"
    },
    "require-dev": {
        "illuminate/database": "4.1.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-0": {
            "Illuminate\\Validation": ""
        },
	"psr-4": {
	    "App\\": "app/"
	}

    },
    "target-dir": "Illuminate/Validation",
    "minimum-stability":"dev"
}
